{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main",
    "overlay",
    "settings"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "global-shortcut:default",
    "shell:default",
    "notification:allow-show",
    "autostart:allow-enable",
    "autostart:allow-disable",
    "autostart:allow-is-enabled",
    "store:default"
  ]
}